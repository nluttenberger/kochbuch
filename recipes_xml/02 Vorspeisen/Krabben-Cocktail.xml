    <fr:recipeName>Krabben-Cocktail</fr:recipeName>
    <fr:recipeKeywords>Krabben,Birnen,Kiwis,Walnuss</fr:recipeKeywords>
    <fr:recipeIntro>
        <fr:introText>Besonders lecker zum Frühstück.</fr:introText>
    </fr:recipeIntro>
    <fr:recipeIngredients>
        <fr:igdtList>
            <fr:igdtListName>fest</fr:igdtListName>
            <fr:igdtListLine>
