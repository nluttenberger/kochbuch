<fr:recipeName>Krabben-Cocktail</fr:recipeName><fr:recipeKeywords>Krabben,Birnen,Kiwis,Walnuss</fr:recipeKeywords><fr:recipeIntro><fr:introText>Besonders lecker zum Frühstück.</fr:introText></fr:recipeIntro><fr:recipeIngredients><fr:igdtList><fr:igdtListName>fest</fr:igdtListName><fr:igdtListLine><fr:igdtQuantity/><fr:igdtName ref="krabben">Krabben</fr:igdtName></fr:igdtListLine><fr:igdtListLine><fr:igdtQuantity/><fr:igdtName ref="birne">reife Birne</fr:igdtName></fr:igdtListLine><fr:igdtListLine><fr:igdtQuantity/><fr:igdtName ref="kiwi">reife Kiwi</fr:igdtName></fr:igdtListLine><fr:igdtListLine><fr:igdtQuantity/><fr:igdtName ref="walnuss">Walnüsse</fr:igdtName></fr:igdtListLine></fr:igdtList><fr:igdtList><fr:igdtListName>flüssig</fr:igdtListName><fr:igdtListLine><fr:igdtQuantity>1 EL</fr:igdtQuantity><fr:igdtName ref="mayonnaise">Miracel Whip</fr:igdtName></fr:igdtListLine><fr:igdtListLine><fr:igdtQuantity>2 EL</fr:igdtQuantity><fr:igdtName ref="joghurt">Joghurt 3,5% Fett</fr:igdtName></fr:igdtListLine><fr:igdtListLine><fr:igdtQuantity>1</fr:igdtQuantity><fr:igdtName ref="knoblauch">Knoblauchzehe</fr:igdtName></fr:igdtListLine><fr:igdtListLine><fr:igdtQuantity/><fr:igdtName ref="dill">Dill</fr:igdtName></fr:igdtListLine><fr:igdtListLine><fr:igdtQuantity/><fr:igdtName ref="worcester">Worcestershire-Sauce (sparsam)</fr:igdtName></fr:igdtListLine><fr:igdtListLine><fr:igdtQuantity/><fr:igdtName ref="sherry">Sherry</fr:igdtName></fr:igdtListLine><fr:igdtListLine><fr:igdtQuantity/><fr:igdtName ref="zitrone">Zitronensaft</fr:igdtName></fr:igdtListLine><fr:igdtListLine><fr:igdtQuantity/><fr:igdtName ref="">Salz, Pfeffer</fr:igdtName></fr:igdtListLine></fr:igdtList><fr:igdtList><fr:igdtListName>garnieren</fr:igdtListName><fr:igdtListLine><fr:igdtQuantity>1</fr:igdtQuantity><fr:igdtName ref="limette">Limette</fr:igdtName></fr:igdtListLine></fr:igdtList></fr:recipeIngredients><fr:recipeInstructions><fr:instruction><fr:instrStepName>Zubereitung</fr:instrStepName><fr:instrStepT